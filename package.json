{
  "name": "strapi-next",
  "private": true,
  "version": "1.0.0",
  "description": "Full-stack edtech demo with Strapi (cms) and Next.js (web)",
  "license": "MIT",
  "scripts": {
    "dev:cms": "pnpm --filter cms dev",
    "dev:web": "pnpm --filter web dev",
    "dev": "concurrently \"pnpm dev:cms\" \"pnpm dev:web\"",

    "playwright:test": "pnpm exec playwright test",
    "playwright:ui": "pnpm exec playwright test --ui",

    "test:e2e": "pnpm exec playwright test",
    "test:e2e:ui": "pnpm exec playwright test --ui"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20",
    "concurrently": "^9.0.0"
  },
  "packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca"
}
